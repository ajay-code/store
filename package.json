{
    "name": "store",
    "version": "1.2.0",
    "description": "A site to add, review and rate stores",
    "main": "./dist/app.js",
    "type": "module",
    "engines": {
        "node": ">=16.10.0"
    },
    "scripts": {
        "test": "cross-env NODE_OPTIONS=--experimental-vm-modules npx jest src/",
        "start": "node dist/server.js",
        "build": "tsc",
        "dev": "concurrently \"tsc -w\" \"npm:nodemon\"",
        "nodemon": "nodemon -w dist dist/server.js",
        "gen:key": "node dist/scripts/genKey.js",
        "db:migrate": "knex migrate:latest --knexfile dist/lib/knex/knexFile.js",
        "db:rollback": "knex migrate:rollback --knexfile dist/lib/knex/knexFile.js",
        "db:seed": "knex seed:run --knexfile dist/lib/knex/knexFile.js"
    },
    "keywords": [],
    "author": "Ajay Singh",
    "license": "ISC",
    "imports": {
        "#src/*": "./dist/*"
    },
    "dependencies": {
        "bcrypt": "^5.1.0",
        "cookie-parser": "^1.4.6",
        "cors": "^2.8.5",
        "dotenv": "^16.0.3",
        "express": "^4.18.2",
        "express-async-errors": "^3.1.1",
        "helmet": "^6.0.1",
        "http-status": "^1.6.2",
        "jsonwebtoken": "^9.0.0",
        "knex": "^2.3.0",
        "lodash-es": "^4.17.21",
        "multer": "^1.4.5-lts.1",
        "mysql2": "^3.0.1",
        "nodemailer": "^6.9.0",
        "sharp": "^0.31.3",
        "zod": "^3.20.2"
    },
    "devDependencies": {
        "@types/bcrypt": "^5.0.0",
        "@types/cookie-parser": "^1.4.3",
        "@types/cors": "^2.8.13",
        "@types/express": "^4.17.14",
        "@types/jest": "^29.2.5",
        "@types/jsonwebtoken": "^9.0.0",
        "@types/lodash-es": "^4.17.6",
        "@types/multer": "^1.4.7",
        "@types/nodemailer": "^6.4.7",
        "@types/sharp": "^0.31.1",
        "concurrently": "^7.6.0",
        "cross-env": "^7.0.3",
        "jest": "^29.3.1",
        "madge": "^5.0.1",
        "nodemon": "^2.0.20",
        "ts-jest": "^29.0.3",
        "typescript": "^4.9.3"
    }
}
